# Production Environment Variables for Vercel
# These will be set in Vercel dashboard

# API Configuration - will point to deployed backend
REACT_APP_API_BASE_URL=https://your-backend-deployment.vercel.app

# Application Settings
REACT_APP_ENVIRONMENT=production

# Feature Flags
REACT_APP_ENABLE_DEBUG=false
REACT_APP_ENABLE_ANALYTICS=true

# Map Configuration
REACT_APP_DEFAULT_MAP_CENTER_LAT=37.7749
REACT_APP_DEFAULT_MAP_CENTER_LNG=-122.4194
REACT_APP_DEFAULT_MAP_ZOOM=12
